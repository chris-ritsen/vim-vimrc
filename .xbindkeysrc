"xmodmap ~/.xmodmap"
  m:0x0 + c:169
  # XF86Eject

# ""
#   m:0x0 + c:212
  #XF86LaunchB

"nohup exec chromium"
  m:0x0 + c:194

"amixer -c0 sset Master toggle"
  m:0x0 + c:121
  # XF86AudioMute


"amixer -c0 set Master 1- unmute"
  m:0x0 + c:122
  # XF86AudioLowerVolume

"amixer -c0 set Master 1+ unmute"
  m:0x0 + c:123
  # XF86AudioRaiseVolume

# "(Scheme function)"
    # m:0x10 + c:77
    # Mod2 + Num_Lock

"eject"
  m:0x0 + c:195

"eject -t"
  m:0x0 + c:196

